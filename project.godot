; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Mossism-qodot"
run/main_scene="res://nodes/menus/StartMenu.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Mossism/"
config/features=PackedStringArray("4.0", "C#", "Forward Plus")
boot_splash/bg_color=Color(0.141176, 0.141176, 0.141176, 0)
boot_splash/use_filter=false
config/icon="res://icon.svg"

[audio]

driver/mix_rate.web=44100

[autoload]

Level="*res://scripts/autoloads/level.gd"
Keyboard="*res://scripts/autoloads/keyboard.gd"
Controller="*res://scripts/autoloads/controller.gd"
SceneManager="*res://addons/scene_manager/scene_manager.tscn"
Scenes="*res://addons/scene_manager/scenes.gd"
InputRemapper="*res://scripts/autoloads/InputRemapper.gd"
Keycodes="*res://scripts/autoloads/keycodes.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/subwindows/embed_subwindows=false
window/stretch/mode="canvas_items"
window/stretch/aspect="ignore"

[dotnet]

project/assembly_name="Mossism-qodot"

[editor_plugins]

enabled=PackedStringArray("res://addons/scene_manager/plugin.cfg", "res://addons/qodot/plugin.cfg")

[gui]

fonts/canvas_textures/default_texture_filter=0

[layer_names]

3d_physics/layer_1="ground"
3d_physics/layer_2="walls"
3d_physics/layer_3="moving"
3d_physics/layer_17="weapon-1"
3d_physics/layer_18="weapon-2"
3d_physics/layer_19="weapon-3"
3d_physics/layer_20="weapon-4"
3d_physics/layer_21="player-1"
3d_physics/layer_22="player-2"
3d_physics/layer_23="player-3"
3d_physics/layer_24="player-4"

[rendering]

textures/canvas_textures/default_texture_filter=0
environment/defaults/default_clear_color=Color(0.301961, 0.301961, 0.301961, 0)
occlusion_culling/use_occlusion_culling=true
